#include<bits/stdc++.h>
using namespace std;
int main()
{
   int t;
   cin>>t;
   t=t*2+1;
   while(t--)
   {
      char s;
      scanf("%c",&s);
      if(s=='B'|| s=='b')
      {
         cout<<"BattleShip"<<endl;
      }
      if(s=='c'||s=='C')
      {
         cout<<"Cruiser"<<endl;
      }
      if(s=='D'||s=='d')
      {
         cout<<"Destroyer"<<endl;
      }
      if(s=='f'||s=='F')
      {
         cout<<"Frigate"<<endl;
      }
   }
}
